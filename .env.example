# Supabase Configuration
# Get these from your Supabase project: Settings > API
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Cloudinary Configuration
# Get these from your Cloudinary dashboard: Settings > Security
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
NEXT_PUBLIC_CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret

# Cloudinary Upload Preset
# Create an unsigned upload preset in Cloudinary: Settings > Upload > Upload presets
# Name it "course_videos" or set a custom name here
NEXT_PUBLIC_CLOUDINARY_UPLOAD_PRESET=course_videos
